	});&#xD;
}else{&#xD;
	var i = 1;&#xD;
	var j = 0;&#xD;
	yxtks.each(function(yxtk){&#xD;
		if(i != yxtk.get(&quot;id&quot;) &amp;&amp; j == 0){&#xD;
			j++;&#xD;
