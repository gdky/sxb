<?xml version="1.0" encoding="UTF-8"?>
<ViewConfig>
  <Arguments/>
  <Context/>
  <Model>
    <DataType name="zsdtsjlInfoType">
      <PropertyDef name="tsrq">
        <Property name="dataType">Date</Property>
      </PropertyDef>
      <PropertyDef name="ms">
